/*保障范围*/
._prompt_form1 label{display: block; float: left; height: 64px;}
._prompt_form1 ._insurance_label_left{width: 97%;float:left; position: relative;}
._prompt_form1 ._insurance_label_left p._insurance_name{font-size:18px; line-height:22px;margin:12px 80px 0 9px;}
._prompt_form1 ._insurance_label_left p._insurance_explain{color:#999999;font-size:14px;line-height:28px;margin-left:9px;}
._prompt_form1 ._insurance_label_left p._prompt_force_flag{font-size: 14px; color: #da535d; line-height:28px;margin-left:50px;}
._prompt_form1 ._insurance_label_right{width:3%;float:left;}
._prompt_form1 ._insurance_label_right img{
	margin-right:5px;
	margin-top:7px;
	float:right;
	-webkit-transition:0.3s;
	-ms-transition:0.3s;
	-moz-transition:0.3s;
	-o-transition:0.3s;
	transition:0.3s;
}
._prompt_form1 ._insurance_label_right img:hover{
	margin-right:5px;
	margin-top:7px;
	float:right;
	-webkit-transition:0.3s;
	-ms-transition:0.3s;
	-moz-transition:0.3s;
	-o-transition:0.3s;
	transition:0.3s;
	-webkit-transform:rotate(90deg);
	-ms-transform:rotate(90deg);
	-moz-transform:rotate(90deg);
	-moz-transform:rotate(90deg);
	transform:rotate(90deg);
}
