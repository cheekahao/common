@charset "utf-8";
/*--投保有礼--*/
#slides{position: relative;}
#slides img{width: 100%; display: block;}
._insurance_box{background: #fff; }
._insurance_box ._insurance_list{ border-bottom: 1px solid #b9b9b9;  padding-left: 5px; padding-right: 5px; overflow: hidden; }
._insurance_box ._insurance_list ul li{float: left;}
._insurance_box ._insurance_list ul ._left_logo{width: 24%; height: 88px;   border-right: 1px dashed #b9b9b9; text-align: center; padding-top: 14px;}
._insurance_box ._insurance_list ul ._left_logo img{width: 70px;}
._insurance_box ._insurance_list ul ._right_news{width: 75%;height: 102px; background: url(../images/_fh_icon.png) no-repeat right; background-size: 18px;display: table; }
._insurance_box ._insurance_list ul ._right_news ._right_list{padding-left: 5px; color: #000; width: 88%; height: 102px;display: table-cell; vertical-align: middle;}
._insurance_box ._insurance_list ul ._right_news ._right_list span{color: #e4393c;}
._insurance_box ._insurance_list ul ._right_news ._right_list ._zw_news{color: #666;}
._insurance_box ._insurance_list ul ._right_news ._right_list ._zw_news a {color: #666;}
._insurance_box ._insurance_list ul ._right_news ._right_list a{color: #000}
@media only screen and (min-width: 414px) and (max-width: 1000px){
    ._insurance_box ._insurance_list ul ._right_news ._right_list{font-size: 16px;}
   
}
@media only screen and (max-width: 320px) and (max-width: 375px){
    ._insurance_box ._insurance_list ul ._right_news ._right_list{font-size: 12px;}
}
._gift_title{ background: #eee; height: 35px; line-height: 35px;}
._gift_title span{margin-left: 20px; color: #d02f32; font-size: 18px;}

._event_details_box{background: #fff; }
._event_details_box ._event_details{padding: 19px 10px 19px 10px;}
._event_details_box ._event_details_news{color: #000;font-size: 16px; }
._event_details_box ._event_details_news p{color: #000; height: 30px;}
._event_details_box ._event_details_news ul li{ color: #333; line-height: 35px;}
._event_details_box ._event_details_news ul li span{color: #e4393c;}
._event_details_box ._event_details_news ._detail{margin-top: 15px;}
._event_details_box ._event_details_news ._detail li{color: #666;}
._event_details_box ._event_details ._insure_box{    margin-top: 10px;}
._event_details_box ._event_details ._insure_box ._insure_button{height: 43px; width: 100%; font-family: "微软雅黑"; border: none;cursor: pointer; background: url(/stc/images/_button_bj.jpg);  line-height: 43px;text-align: center; color: #fff;font-size: 18px;border-radius: 5px;}

@media only screen and (min-width: 375px) and (max-width: 1000px){
    ._event_details_box ._event_details_news{font-size: 16px;}
   
}
@media only screen and (max-width: 320px){
    ._event_details_box ._event_details_news{font-size: 14px;}
}


@media only screen and (min-width: 375px) and (max-width: 1000px){
    ._event_details_box ._event_details{font-size: 16px;}
   
}
@media only screen and (max-width: 320px){
    ._event_details_box ._event_details{font-size: 14px;}
}
/*about_us*/
._about_us_details{text-align: left;font-size: 15px;line-height: 26px;}
._about_us_details section{min-width: 100px;}
._about_us_details h1{color: #000;padding: 0 15px;padding-top: 20px;}
._about_us_details p{color: #666;padding: 0 15px;}

._about_us_details p._indent{text-indent: 30px;}
._about_us_details ._cooperative_partner{margin-top:10px;margin-bottom: 50px;}
