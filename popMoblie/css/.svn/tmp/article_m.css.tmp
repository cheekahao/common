/*article_list*/
._article_ul_mobile{padding-left: 10px;padding-right: 10px;	background: #FFFFFF;}
._article_ul_mobile li{border-bottom: 1px #EEEEEE solid;}
._article_ul_mobile li img{padding: 10px 0;	display: block;	vertical-align: middle;	width: 104px;margin-right: 10px;float: left;}
._article_ul_mobile li h1{padding-top: 10px;font-size: 16px;line-height: 1.5;font-weight: normal;overflow: hidden;height: 48px;}
._article_ul_mobile time{float: left;margin-top: 6px;font-size: 12px;color: #c3c3c3;}
._article_ul_mobile li a{color: #666666;}
._pages_div{font-size: 12px; background: #FFFFFF; padding: 10px; text-align: center;}
._pages_div ._pages_list{}
._pages_div ._pages_list li{display: inline; margin-right: 6px;}
._pages_div ._pages_list li a{color: #666666; padding: 2px 4px;}
._pages_div ._pages_list li a:hover{background: #EEEEEE;}
._pages_div ._pages_list li._page_list_on{color: #000000; padding: 2px 4px;}
/*article_details*/
._article_ditails_moblie{margin: 10px; background: #FFFFFF;}
._article_ditails_moblie header, section, footer{background: none; height: auto;}
._article_ditails_moblie header{margin-bottom: 20px;}
._article_ditails_moblie header h1{text-align:left;line-height: 28px; font-size: 18px; margin: 10px 10px 0 10px; color: #333333;}
._article_ditails_moblie header p{font-size: 12px; color: #999999; text-align:left; margin: 10px 0px 0 10px;}
._article_ditails_moblie section{margin-bottom: 20px;}
._article_ditails_moblie section p{padding: 4px 10px; color: #666666; line-height: 24px;}
._article_ditails_moblie section p.abstract{padding: 4px 20px; color: #999999; font}
._article_ditails_moblie footer{font-size: 12px; line-height: 24px; color: #3f3f3f;}
._article_ditails_moblie footer h1{font-weight: normal; color: #000000; margin: 0 10px; line-height: 20px; font-size: 18px;}
._article_ditails_moblie footer h1 i{color: #c6c6c6; font-size: 14px; margin-left: 2px;}
._article_ditails_moblie footer ul{margin: 10px 10px 20px 10px; margin-bottom: 20px;}
._article_ditails_moblie footer ul li img{margin-top:-10px;height:15px;}
._article_ditails_moblie footer ul li a{color:#323232;font-size:15px;}
