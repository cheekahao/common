html{font-family: "微软雅黑";}
*{-webkit-box-sizing:border-box;box-sizing:border-box;-moz-box-sizing:border-box;-o-box-sizing:border-box;-ms-box-sizing:border-box;}
body{padding:0;margin:0;}
a{text-decoration: none;}
ul{
	list-style: none;
}


.contain{
	width:100%;
	min-width: 320px;
	max-width: 1000px;
	margin:auto;
}
._m_header{
    width:100%;
	min-width: 320px;
	max-width: 1000px;;
    height:64px;
    background:#fff;
}
._m_header .left{
    float:left;
    margin-left:20px;
    margin-top:15px;
}
._m_header .right{
    float:right;
     margin-right:20px;
     background:red;
     margin-top:10px;
}
._m_header .left ._m_icon_left{
  float:left;
}
.title {
	width: 155px;
	height: 40px;
	float: left;
	text-align: center;
	line-height: 40px;
	
}
._m_header .left .tel{
    text-align:center;
    color:#1e6a9b;
	font-size:20px;
	float:right;
	}
._m_header ._m_icon_right{
   float:left;
	
}
