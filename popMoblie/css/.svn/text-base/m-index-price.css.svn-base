html{font-family: "微软雅黑";}
body{padding:0;margin:0;}
a{text-decoration: none;}
ul{
	list-style: none;
}


.contain{
	width:100%;
	min-width: 320px;
	max-width: 1000px;
	margin:auto;
}
._m_header{
    width:100%;
	min-width: 320px;
	max-width: 1000px;;
    height:64px;
    background:#fff;
}
._m_header .left{
    float:left;
    margin-left:20px;
    margin-top:15px;
}
._m_header .right{
    float:right;
     margin-right:20px;
     background:red;
     margin-top:10px;
}
._m_header .left ._m_icon_left{
  float:left;
}
.title {
	width: 155px;
	height: 40px;
	float: left;
	text-align: center;
	line-height: 40px;
	
}
._m_header .left .tel{
    text-align:center;
    color:#1e6a9b;
	font-size:20px;
	float:right;
	}
._m_header ._m_icon_right{
   float:left;
	
}
._shop_xian{
	width:100%;
	max-width:1000px;
	min-width: 320px;
	height:47px;
	background:#11b2b0;
	float:left;
}
._shop_xian ._bijia_results{
   margin-left:20px;
   margin-right:20px;
   height:36px;
  
}
._shop_xian ._bijia_results ._bijia_results_right{
  float:right;
  margin-top:12px;
}
._shop_xian ._bijia_results ._bijia_results_right span{
  color:#fff;
  font-size:13px;
   
}
._shop_xian p{
	font-size:13px;
	color:#fff;
	display:inline-block;
	
} 
._all_xian{
	width:100%;
	max-width:1000px;
	min-width: 320px;
	background:#fff;
}
._all_xian ul{
	width:100%;
	margin:0;
	padding:0;
	
}
._all_xian ul li{
	float:left;
	width:100%;
	height:66px;
	background:#fff;
	border-top:1px solid #d9d9d9;
}
._all_xian ul li._se_zhi{
	background:#edf9f8;
}
/*险种左边的内容*/
._all_xian ul li ._kinds_xian_left{
	margin-left:4px;
	width:50px;
	height:45px;
	border-right:1px dashed #d2d2d2;
	margin-top:10px;
	float:left;
}
._all_xian ul li ._kinds_xian_left ._m_logo{
	width:50px;
	height:45px;
	vertical-align: middle;
	text-align: center;
}
._all_xian ul li ._kinds_xian_left ._m_logo img{
	width:45px;
	height:45px;
}
._all_xian ul li ._kinds_xian_right{
	margin-right:2px;
	float:left;
	margin-left:6px;
	height:66px;
}
._all_xian ul li ._kinds_xian_right ._define{
	float:left;
	margin-top:10px;
}
._all_xian ul li ._kinds_xian_right ._define p._price_se{
color:#11b2b0;
font-size:19px;
}
._price_se{
	color:#11b2b0;
}
._all_xian ul li ._kinds_xian_right ._define p{
	margin:0;
	padding:0;
	color:#888888;
	font-size:15px;
}
._all_xian ul li ._kinds_xian_right ._define span{
	color:#888888;
	font-size:15px;
}
._all_xian ul li ._kinds_xian_right ._define span a{
	color:#ff9e05;
	font-size:15px;
}
._floatRight{
	float:right;
	margin-top:15px;
}
