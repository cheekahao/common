@charset "utf-8";
body,ul,li {
	padding:0;
	margin:0;
	border:0;
	list-style:none;
}
body {
	font-size:12px;
	-webkit-user-select:none;
    -webkit-text-size-adjust:none;
	font-family:helvetica;
