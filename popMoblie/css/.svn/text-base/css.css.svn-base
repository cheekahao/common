	-webkit-tap-highlight-color: transparent;
	-webkit-appearance: none;
}
/*--方案选择--*/
.header{
	width: 100%;
	height: 45px;
	position: absolute;;
	z-index: 2px;
	line-height: 45px;
	background-color:#d51875;
	background-image:-webkit-gradient(linear, 0 0, 0 100%, color-stop(0, #e4393c), color-stop(0.05, #e4393c), color-stop(1, #e4393c));
	background-image:-moz-linear-gradient(top, #e4393c, #e4393c 5%, #e4393c);
	background-image:-o-linear-gradient(top, #e4393c, #e4393c 5%, #e4393c);
	color: #fff;
	text-align: center;
	font-size: 2.0em;

}
.header img{
	float: right;
	width: 30px;
	padding-top: 8px;
	margin-right: 8px;
	vertical-align: middle;
}
.header span img{
	width: 18px;
	float: left;
	vertical-align: middle;
	padding-top: 7px;
	margin-left: 5px;

}
.program-wrap{
	width: 100%;
	background-color: #fff;
}
	.menu{
		width: 100%;
		height: 53px;
		
	}
	.menu ul li{
		width: 50%;
		height: 45px;
		float: left;
		background-color: #a7a7a7;
		overflow: hidden;
		cursor: pointer;
		

	}
	.menu ul li a{
		background:url(../images/border_img.jpg) no-repeat right top; 
		height: 45px;
		float: left;
		display:block;
		font-size: 1.5em;
		color: #fff;
		line-height: 45px;
		text-align: center;
		width: 100%;

	}
	.menu ul li span{ 
		height: 45px;
		float: left;
		display:block;
		font-size: 1.5em;
		color: #fff;
		line-height: 45px;
		text-align: center;
		width: 100%;
	}
	.menu ul li.hover{
		width: 50%;
		height: 59px;
		background: url(../images/arrow.png) no-repeat center bottom;
		
		

	}
	.menu img{
		width: 30px;
		position: absolute;
		top: 54px;
	}
	.list{
		width: 100%;
		color: #B8B8B8;
		font-size: 1.5em;
		border-bottom: 1px solid #DADADA;
		margin-top: 8px;
		

	}
	.list span{
		margin-left: 10px;
	}
	.list p{
		color: #000;
		margin-top: 10px;
		margin-left: 10px;
	}
